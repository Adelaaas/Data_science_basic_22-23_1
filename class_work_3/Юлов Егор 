import numpy as np
a = np.array ([[1,2,4], [1,1,1], [2,5,6], [1,4,2], [3,5,1]], int)
b = a[0, 0]**2 + a[0, 1]**2 + a[0, 2]**2
b1 = np.sqrt (b)
c = a[1, 0]**2 + a[1, 1]**2 + a[1, 2]**2
c1 = np.sqrt (c)
d = a[2, 0]**2 + a[2, 1]**2 + a[2, 2]**2
d1 = np.sqrt (d)
e = a[3, 0]**2 + a[3, 1]**2 + a[3, 2]**2
e1 = np.sqrt (e)
f = a[4, 0]**2 + a[4, 1]**2 + a[4, 2]**2
f1 = np.sqrt (f)
h = max(b1,c1,d1,e1,f1)
print (h)