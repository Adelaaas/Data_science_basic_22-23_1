        "def is_prime(n):\n",
        "  f = True\n",
        "  for i in range(2, int(n ** 0.5) + 1):\n",
        "    if n % i == 0:\n",
        "      f = False\n",
        "  return f\n",
        "\n",
        "print(is_prime(7))\n",
        "print(is_prime(10))"
      ]
    }
